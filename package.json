{
  "name": "imuter",
  "version": "0.3.0",
  "description": "Immutable data library",
  "homepage": "https://github.com/jbedard/imuter",
  "keywords": [
    "immutable",
    "freeze",
    "frozen",
    "onpush",
    "observable"
  ],
  "main": "dist/imuter.js",
  "types": "dist/imuter.d.ts",
  "side-effects": false,
  "author": "Jason Bedard",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jbedard/imuter.git"
  },
  "devDependencies": {
    "@types/jasmine": "3.3",
    "@types/node": "8.0",
    "conventional-changelog-cli": "2.0",
    "dtslint": "0.7",
    "istanbul": "0.4",
    "jasmine": "3.4",
    "jasmine-core": "3.4",
    "karma": "4.1",
    "karma-chrome-launcher": "2.2",
    "karma-coverage": "1.1",
    "karma-jasmine": "2.0",
    "karma-typescript": "4.1",
    "tslint": "5.11",
    "typescript": "3.5",
    "uglify-es": "3.3"
  },
  "scripts": {
    "build": "yarn lint && yarn dtslint && yarn test && yarn compile && yarn uglify",
    "clean": "rm -Rf dist coverage",
    "compile": "tsc",
    "uglify": "uglifyjs -c -m --toplevel --define process.env.NODE_ENV='\"production\"' --define FREEZING_ENABLED=false --source-map 'content=dist/imuter.js.map' -o dist/imuter.min.js dist/imuter.js",
    "test": "karma start karma.conf.js --single-run --browsers ChromeHeadless",
    "test-server": "karma start karma.conf.js",
    "dtslint": "dtslint type-specs",
    "lint": "tslint -c ./tslint.json --project ./tsconfig.json",
    "changelog": "conventional-changelog --infile ./CHANGELOG.md --same-file --release-count 1 --preset angular"
  }
}
